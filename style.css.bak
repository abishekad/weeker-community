    @media print {

        .top-bar,
        .main-nav,
        .social-icons,
        .back-to-top,
        .hero-buttons,
        footer {
            display: none !important;
        }

        body {
            background: white;
            color: black;
        }

        .hero {
            page-break-after: always;
        }

        .section {
            page-break-inside: avoid;
        }

        a {
